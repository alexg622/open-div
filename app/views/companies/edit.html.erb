<div>
	<%= form_for @company, method: "put" do |f| %>

	 <%= f.label :name %><br />
    <%= f.text_field :name %>

    <%= f.label :short_description %><br />
    <%= f.text_field :short_description %>

    <%= f.label :domain %><br />
    <%= f.text_field :domain %>

    <%= f.label :city_name %><br />
    <%= f.text_field :city_name %>

    <%= f.label :region_name %><br />
    <%= f.text_field :region_name %>

    <%= f.label :country_code %><br />
    <%= f.text_field :country_code %>

    <%= f.label :profile_image_url %><br />
    <%= f.text_field :profile_image_url %>

    <%= f.label :creator_id %><br />
    <%= f.text_field :creator_id %>

    <%= f.submit "Update Company" %>
    <% end %>
  
</div>



