<div class="row">
    <div class="col s12">
      <div class="row">
        <div class="input-field col s12 wow fadeIn" data-wow-duration="1.5s" data-wow-delay="1.5s">
          <form action="<%= search_path %>" method="get" data-turbolinks="false">
            <input type="search" name="search" id="autocomplete" placeholder="Search for a Company" class="autocomplete" autocomplete="off">
            <label class="label-icon" for="autocomplete"><i class="material-icons">search</i></label>
          </form>
        </div>
      </div>
    </div>
  </div>
